{
  "name": "api-node-express",
  "version": "1.0.0",
  "description": "API RESTful para gerenciamento de livraria com Node.js, Express e MongoDB",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:watch": "node --experimental-vm-modules node_modules/jest/bin/jest.js --watch",
    "test:coverage": "node --experimental-vm-modules node_modules/jest/bin/jest.js --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "audit": "npm audit --audit-level=high",
    "prepare": "echo 'Ready to commit'"
  },
  "keywords": [
    "api",
    "rest",
    "express",
    "mongodb",
    "mongoose",
    "nodejs"
  ],
  "author": "ESousa97",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ESousa97/api-node-express-2.git"
  },
  "bugs": {
    "url": "https://github.com/ESousa97/api-node-express-2/issues"
  },
  "homepage": "https://github.com/ESousa97/api-node-express-2#readme",
  "devDependencies": {
    "@eslint/js": "^9.0.0",
    "eslint": "^9.0.0",
    "globals": "^15.0.0",
    "jest": "^29.7.0",
    "nodemon": "^3.0.0"
  },
  "dependencies": {
    "dotenv": "^16.3.0",
    "express": "^4.18.2",
    "mongoose": "^8.0.0",
    "mongoose-autopopulate": "^1.1.0"
  }
}
